background_y[3]=-view_yview*.2
background_y[1]=-y/2
background_y[2]=-y/2
if y&lt;view_yview+view_hview/3
{
    var i=view_yview+view_hview/3-y;
    view_yview=max(-boss*20000,view_yview-i,view_yview-10)
    score=-view_yview/10
if score&lt;2000-view_hview/10
{
    var ilos=random(100*max(1,(5-abs(view_yview/1000))));
    if ilos&lt;1
    {
        i=instance_create((random(room_width-20*4)) div 10*10,(view_yview-300) div 10 *10,objSciana)
        i.size=16
        i.image_xscale=i.size
        i.image_yscale=i.size
    }else if ilos&lt;3
    { 
        i=instance_create(0,(view_yview-300) div 10 *10,objSciana)
        i.size=4
        i.image_xscale=i.size
        i.image_yscale=i.size
        i=instance_create(view_wview-4*10,(view_yview-300) div 10 *10,objSciana)
        i.size=4
        i.image_xscale=i.size
        i.image_yscale=i.size
    }else if ilos&lt;6
    {
        var ix=0
        var isize=choose(4,16)
        var ixx=(random(room_width-20*4)) div 10*10
        var iyy=(view_yview-isize*10*3) div 10 *10
        repeat(1+random(3)div 1)
        {
            var iy=0
            repeat(1+random(3)div 1)
            {
                if random(100)&lt;70
                {
                i=instance_create(ixx+ix*isize*10,iyy+iy*isize*10,objSciana)
                i.size=isize
                i.image_xscale=i.size
                i.image_yscale=i.size
                iy+=1
                }
            }
            ix+=1
        }
    }else if ilos&lt;10
    {
        i=instance_create((random(room_width-20*4)) div 10*10,(view_yview-300) div 10 *10,objSciana)
        i.size=4
        i.image_xscale=i.size
        i.image_yscale=i.size
    }
}else if score&gt;=2000-view_hview/30 and !instance_exists(objBoss1) and boss=1
{
//boss 1
//tworzenie poszczeglnych elementow bossa 1
var wspolczynnik=1;
repeat(2)
{
////kurwa nie chce mi sie dzisiaj, trzeba tu pierdolnąć jakiegoś potforka czy cuś ...
    instance_create((view_xview+view_wview/2)div 10 *10+10*4.5*wspolczynnik,view_yview-view_hview/3+40,objBoss1)
    instance_create((view_xview+view_wview/2)div 10 *10+10*3.5*wspolczynnik,view_yview-view_hview/3+(40+10),objBoss1)
    var xxx=5, yyy=0;
    repeat(3)
    {
        repeat(3)
        {
            if xxx!=15 or yyy!=10
            instance_create((view_xview+view_wview/2)div 10 *10+xxx*wspolczynnik,view_yview-view_hview/3+(40+20)+yyy,objBoss1)
            xxx+=10
        }
        xxx=5
        yyy+=10
    }
wspolczynnik=-1
}
with(objBoss1) life=50
//niszczenie pozostałości ścian
with(objSciana)
{
    part_odlamek_create(x,y)
    instance_destroy()
}
//pasek bossa
instance_create(0,view_yview,objBoss_bar)
}
else if score&gt;=4000-view_hview/30 and !instance_exists(objBoss2) and boss=2
{
//boss 2
instance_create((view_xview+view_wview/2)div 10 *10-4*5,view_yview-view_hview/3+40,objBoss2)
instance_create(0,view_yview,objBoss_bar)
