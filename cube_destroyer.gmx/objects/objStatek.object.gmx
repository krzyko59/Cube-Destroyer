            <string>///sterowanie
if os_windows=0 //and !joystick_exists(0)
{
switch(global.sterowanie)
{
case 0:
    if objPad.mouse_id!=-1
    {
        direction=objPad.direction
        ster_dziala=1
    }
    else ster_dziala=0
break
case 1:
    if mouse_check_button(mb_any)
    {
        direction=point_direction(view_xview+view_wview/2,view_yview+view_hview/2,mouse_x,mouse_y)
        ster_dziala=1
    }
    else ster_dziala=0
break
case 2:
    direction=point_direction(0,0,-device_get_tilt_x(),device_get_tilt_y())
    if mouse_check_button(mb_any) ster_dziala=1
    else ster_dziala=0
break
}
}else
{

if joystick_xpos(1)&lt;0.3 and joystick_xpos(1)&gt;-0.3 and joystick_ypos(1)&lt;0.3 and joystick_ypos(1)&gt;-0.3
ster_dziala=0
else 
{
    ster_dziala=1
    direction=point_direction(0,0,joystick_xpos(0),joystick_ypos(0))
}
}
//przemieszczanie sie
if pause exit
if ster_dziala and fuel&gt;=1
{
    fuel-=1
    for(ii=-1;ii&lt;=1;ii+=1)
    {
        i=instance_create(min(view_wview-5,max(5,x+lengthdir_x(image_xscale,direction)+random(3)-random(3))),y+lengthdir_y(image_yscale,direction)+random(3)-random(3),objOgien)
        i.direction=direction+random(15)-random(15)
        i.speed=8
    }
    if abs(fps_real)&gt;45
    {
    part_type_direction(p_ogien1,direction,direction,0,0)
    part_emitter_region(ps,em,x-2,x+2,y-2,y+2,0,0)
    part_emitter_burst(ps,em,p_ogien1,1)
    }
    _speed=min(7,_speed+0.3)
}else
{
    y+=1
    _speed=max(0,_speed-1)
}
//
x=max(wielkosc,min(room_width-wielkosc,x-lengthdir_x(_speed,direction)))
y=max(view_yview,y-lengthdir_y(_speed,direction))
//
//
//za malo fuela
if fuel&lt;=0 and alarm[1]&lt;0 and !instance_exists(objOver)
{
    _speed=0
    pause=true
    alarm[1]=45
}
//za nisko polecial
if y&gt;view_yview+view_hview and alarm[1]&lt;0 and !instance_exists(objOver)
{
    _speed=0
    pause=true
    fuel=0
    alarm[0]=-1
    alarm[1]=45
}//generowanie progu
if score&gt;=500+prog*500-view_hview/30
{
    instance_create(0,view_yview,objProg)
    prog+=1
    //generowanie bonusa
    if prog div 2*2=prog
    {   
        var i=instance_create(64+random(room_width-64*2),view_yview-64-random(1000),objBonus);
        if wielkosc=10
        i.typ=2
        else i.typ=1
    }
}
