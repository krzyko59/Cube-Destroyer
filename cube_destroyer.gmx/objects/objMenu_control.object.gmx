            <kind>1</kind>
            <string>//przesuwanie viewa
if abs(cel_x-view_xview)&lt;3 view_xview=cel_x
else view_xview+=(cel_x-view_xview)/3
if abs(cel_y-view_yview)&lt;3 view_yview=cel_y
else view_yview+=(cel_y-view_yview)/3
background_x[1]=view_xview
background_x[2]=view_xview+view_wview-15
if koniec=1
{
    image_alpha-=1/10
    if image_alpha&lt;=0 
    {
        room_goto(rmGame)
    }
}

if mouse_check_button_pressed(mb_any) and view_xview=600 and view_yview=0
{
    //start game
    if device_mouse_y(0)&lt;view_yview+view_hview/8*3+25 and device_mouse_y(0)&gt;view_yview+view_hview/8*3-25
    {
        koniec=1
        with(objMenu_buttony)cel_x=xstart
    }
    //wyniki
    if device_mouse_y(0)&lt;view_yview+view_hview/8*4+25 and device_mouse_y(0)&gt;view_yview+view_hview/8*4-25 and cel_x!=0
    {
        cel_x=0
        for(var i=0;i&lt;=9;i+=1)
        {
        var ii=instance_create(0,view_hview/3+(i*i+1)*100,objMenu_wyniki);
        ii.i=i
        ii.cel_y=view_hview/6*2+40*i
        }
    }
    //options
    if device_mouse_y(0)&lt;view_yview+view_hview/8*5+25 and device_mouse_y(0)&gt;view_yview+view_hview/8*5-25
    {
        cel_x=1200
        instance_create(1200,0,objMenu_options)
    }
    if device_mouse_y(0)&lt;view_yview+view_hview/8*6+25 and device_mouse_y(0)&gt;view_yview+view_hview/8*6-25
    {
        cel_y=view_hview
    }
}
    //ikonka destroy factory
    if mouse_check_button_pressed(mb_any) and device_mouse_x(0)&lt;view_xview+80 and device_mouse_y(0)&lt;view_yview+80
    {
        //url_open("http://destroyfactory.cba.pl/")
    }
if cel_y&gt;=view_hview and !mouse_check_button(mb_any) 
{
cel_y+=3
if cel_y&gt;=view_hview+1040 cel_y=0
}
if keyboard_check_pressed(vk_backspace)
{
    if cel_x!=600 or cel_y!=0
    {
        cel_x=600
        cel_y=0
        with(objMenu_wyniki) cel_y=view_hview+abs(ystart)
    }
    else game_end()
}
</string>
